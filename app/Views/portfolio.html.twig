<!DOCTYPE html>
<html lang="{{app.lang}}">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>{{portfolio.title}} - {{app.site_name}}</title>
	<link rel="stylesheet" href="{{baseUrl()}}/css/screen.css">
	<link rel="stylesheet" href="{{baseUrl()}}/css/font-awesome/css/font-awesome.min.css">
	<link rel="stylesheet" href="{{baseUrl()}}/bxslider/jquery.bxslider.css">
	<meta name="description" content="{{portfolio.seo}}">
</head>
<body>

<!--  - - - - - - - - - - - -HEADER- - - - - - - - - - - - -->
<header class="portfolio-header">
	<div class="container">
		<!-- - - - - - vertical menu - - - - - -->
		<div class="vertical-menu fixed-portfolio-menu" id="portfolio-vertical-menu">
			<div class="vertical-menu-wrapper">
				<div class="logo">
					<img src="{{baseUrl()}}/image/header-nav/logowhite.png" alt="Keef logo">
				</div>
				<div class="lang-vertical">
					<div class="sizer">
						<img src="{{baseUrl()}}/image/vertical-menu/vm.png" alt="passive icon">
					</div>
					<div class="vertical-select">
						<ul class="vert-ul">
							{% for lang in app.langs %}
							<li class="vert-li{% if app.lang == lang %} active-lang{% endif %}"><a href="{{baseUrl()}}/{{lang}}">{{lang}}</a></li>
							{% endfor %}
						</ul>
					</div>
				</div>
			</div>
			<div class="vertical-nav-wrapper">
				<nav class="vertical-nav">
					<div>
						<a href="{{urlFor('home')}}">
							<span class="v-first-li">{{app.texts.home}}</span>
						</a>
					</div>
					<div>
						<a href="{{urlFor('home')}}">
							<span>{{app.texts.what_we_do}}</span>
						</a>
					</div>
					<div>
						<a href="{{urlFor('home')}}">
							<span>{{app.texts.who_we_are}}</span>
						</a>
					</div>
				</nav>
			</div>
		</div>
		<!-- - - - - - end vertical menu - - - - - -->

		<!-- - - - - - SLIDER WRAPPER - - - - - -->
		<div class="slider-wrapper">
			<!-- - - - - - main topic - - - - - -->
			<div class="main-topic">
				<a href="{{urlFor('home')}}">{{portfolio.category.title}}</a>
			</div>
			<!-- - - - - - end main topic - - - - - -->

			<!-- portfolio page -->
			<div class="portfolio-page">

				<!-- main-image -->
				<div class="main-image">
					<img src="{{portfolio.image}}" alt="badaro" class="portfolio-main-image">
					<div class="image-hover-href">
						<a href="{{baseUrl()}}">{{portfolio.category.title}}</a>
					</div>
				</div>
				<!-- end main-image -->

				<!-- main-image-info -->
				<div class="main-image-info">
					<div class="border-container" id="border-container">
						<div class="mini-wrapper">
							<h1>{{portfolio.title}}</h1>
							<h3>{{portfolio.type}}</h3>
							<p class="text-topic">project details:</p>
							<p class="main-text">{{portfolio.description}}</p>
							<p class="date">added {{portfolio.created|date('d/m/Y')}}</p>
							<div class="social">
								{% if portfolio.facebook %}
								<a href="portfolio.facebook">
									<div class="icons">
										<i class="fa fa-facebook"></i>
									</div>
								</a>
								{% endif %}
								{% if portfolio.twitter %}
								<a href="{{portfolio.twitter}}">
									<div class="icons">
										<i class="fa fa-twitter"></i>
									</div>
								</a>
								{% endif %}
								{% if portfolio.google %}
								<a href="{{portfolio.google}}">
									<div class="icons">
										<i class="fa fa-google-plus"></i>
									</div>
								</a>
								{% endif %}
								{% if portfolio.linkedin %}
								<a href="{{portfolio.linkedin}}">
									<div class="icons">
										<i class="fa fa-linkedin"></i>
									</div>
								</a>
								{% endif %}
								{% if portfolio.pinterest %}
								<a href="{{portfolio.pinterest}}">
									<div class="icons">
										<i class="fa fa-pinterest-p"></i>
									</div>
								</a>
								{% endif %}
							</div>
							<div class="pager">
								<a href="{% if portfolio.prev %}{{baseUrl()}}{{urlFor('portfolio', {name:portfolio.prev.token})}}{% else %}javascript:void(0){% endif %}">
									<img src="{{baseUrl()}}/image/slider/arrow/blackleft.png" alt="prev" id="pagerPrev">
								</a>
								<a href="{% if portfolio.next %}{{baseUrl()}}{{urlFor('portfolio', {name:portfolio.next.token})}}{% else %}javascript:void(0){% endif %}">
									<img src="{{baseUrl()}}/image/slider/arrow/blackright.png" alt="next" id="pagerNext">
								</a>
							</div>
						</div>
					</div>
				</div>
				<!-- end main-image-info -->

			</div>
			<!-- end portfolio page -->


		</div>
		<!-- - - - - - END SLIDER WRAPPER - - - - - -->
	</div>
</header>
<!--  - - - - - - - - - - - -END HEADER- - - - - - - - - - - - -->


